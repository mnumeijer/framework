<UserControl x:Class="Signum.Windows.Basics.TypeUI"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows"
    xmlns:d="clr-namespace:Signum.Entities.Basics;assembly=Signum.Entities"
    m:Common.TypeContext="{x:Type d:TypeEntity}"
    MinWidth="600">
    <StackPanel>
        <m:ValueLine m:Common.Route="FullClassName" />
        <m:ValueLine m:Common.Route="CleanName" />
        <m:ValueLine m:Common.Route="TableName" />
    </StackPanel>
</UserControl>
